-- 코드를 작성해주세요
SELECT B.ID AS ID, B.EMAIL AS EMAIL, B.FIRST_NAME AS FIRST_NAME, B.LAST_NAME AS LAST_NAME
FROM SKILLCODES A JOIN DEVELOPERS B ON ((A.CODE & B.SKILL_CODE) > 0)
WHERE A.NAME IN ('Python', 'C#')
GROUP BY B.ID, B.EMAIL, B.FIRST_NAME, B.LAST_NAME
ORDER BY ID

-- OR

SELECT DISTINCT(B.ID) AS ID, B.EMAIL AS EMAIL, B.FIRST_NAME AS FIRST_NAME, B.LAST_NAME AS LAST_NAME
FROM SKILLCODES A JOIN DEVELOPERS B ON ((A.CODE & B.SKILL_CODE) > 0)
WHERE A.NAME IN ('Python', 'C#')
ORDER BY ID