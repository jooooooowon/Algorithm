-- 코드를 작성해주세요
SELECT A.QUARTER, COUNT(*) AS ECOLI_COUNT
FROM
(
    SELECT
        CASE
            WHEN CEIL(MONTH(DIFFERENTIATION_DATE) / 3) = 1 THEN '1Q'
            WHEN CEIL(MONTH(DIFFERENTIATION_DATE) / 3) = 2 THEN '2Q'
            WHEN CEIL(MONTH(DIFFERENTIATION_DATE) / 3) = 3 THEN '3Q'
            WHEN CEIL(MONTH(DIFFERENTIATION_DATE) / 3) = 4 THEN '4Q'
        END AS QUARTER
    FROM ECOLI_DATA
) A
GROUP BY A.QUARTER
ORDER BY A.QUARTER