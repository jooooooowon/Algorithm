-- 코드를 작성해주세요
SELECT B.YEAR_DATE AS 'YEAR', B.MAX_SIZE - A.SIZE_OF_COLONY AS 'YEAR_DEV', A.ID AS 'ID'
FROM ECOLI_DATA A LEFT JOIN
(SELECT MAX(SIZE_OF_COLONY) MAX_SIZE, YEAR(DIFFERENTIATION_DATE) YEAR_DATE
FROM ECOLI_DATA
GROUP BY YEAR(DIFFERENTIATION_DATE)) B ON(YEAR(A.DIFFERENTIATION_DATE) = B.YEAR_DATE)
ORDER BY YEAR, YEAR_DEV